{
    "id": "mcp-miro",
    "name": "MCP Miro",
    "description": "A Model Context Protocol server to connect to the MIRO Whiteboard Application.",
    "tags": [
        "miro",
        "mcp",
        "api"
    ],
    "repo": "https://github.com/evalstate/mcp-miro",
    "command": "npx",
    "baseArgs": [
        "-y",
        "@llmindset/mcp-miro"
    ],
    "configurable": true,
    "configSchema": {
        "properties": {
            "token": {
                "type": "string",
                "description": "MIRO OAuth Token",
                "required": true
            }
        }
    },
    "argsMapping": {
        "token": {
            "type": "env",
            "key": "MIRO_OAUTH_KEY"
        }
    }
}