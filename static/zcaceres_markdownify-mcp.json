{
    "id": "markdownify",
    "name": "Markdownify MCP Server",
    "description": "Markdownify is a Model Context Protocol (MCP) server that converts various file types and web content to Markdown format.",
    "tags": [
        "markdown",
        "conversion",
        "api"
    ],
    "repo": "https://github.com/zcaceres/markdownify-mcp",
    "command": "node",
    "baseArgs": [
        "{ABSOLUTE PATH TO FILE HERE}/dist/index.js"
    ],
    "env": {
        "UV_PATH": "/path/to/uv"
    },
    "configurable": false,
    "overviewFiles": [
        {
            "displayName": "README.md",
            "repoName": "markdownify-mcp",
            "refName": "main",
            "path": "README.md",
            "preferredFileType": "readme",
            "tabName": "README"
        },
        {
            "displayName": "LICENSE",
            "repoName": "markdownify-mcp",
            "refName": "main",
            "path": "LICENSE",
            "preferredFileType": "license",
            "tabName": "MIT"
        }
    ]
}