{
    "id": "mcp-server-motherduck",
    "name": "MCP server",
    "description": "An MCP server for MotherDuck and local DuckDB.",
    "tags": [
        "mcp",
        "server",
        "motherduck",
        "duckdb"
    ],
    "repo": "https://github.com/motherduckdb/mcp-server-motherduck",
    "command": "uvx",
    "baseArgs": [
        "mcp-server-motherduck"
    ],
    "env": {
        "motherduck_token": "",
        "HOME": ""
    },
    "configurable": true,
    "configSchema": {
        "properties": {
            "motherduck_token": {
                "type": "string",
                "description": "MotherDuck access token",
                "required": true
            },
            "HOME": {
                "type": "string",
                "description": "Home directory path",
                "required": true
            }
        }
    },
    "argsMapping": {
        "motherduck_token": {
            "type": "env",
            "key": "MOTHERDUCK_TOKEN"
        },
        "HOME": {
            "type": "env",
            "key": "HOME"
        }
    }
}