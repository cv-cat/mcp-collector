{
    "id": "dataset-viewer",
    "name": "Dataset Viewer",
    "description": "An MCP server for interacting with the Hugging Face Dataset Viewer API, providing capabilities to browse and analyze datasets hosted on the Hugging Face Hub.",
    "tags": [
        "mcp",
        "dataset",
        "huggingface",
        "api"
    ],
    "repo": "https://github.com/privetin/dataset-viewer",
    "command": "uv",
    "baseArgs": [
        "run",
        "dataset-viewer"
    ],
    "env": {
        "HUGGINGFACE_TOKEN": "Your Hugging Face API token for accessing private datasets"
    },
    "configurable": true,
    "configSchema": {
        "properties": {
            "dataset": {
                "type": "string",
                "description": "Dataset identifier (e.g. 'stanfordnlp/imdb')",
                "required": true
            }
        }
    },
    "argsMapping": {
        "dataset": {
            "type": "env",
            "key": "DATASET_IDENTIFIER"
        }
    }
}