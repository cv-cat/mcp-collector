{
    "id": "dify-mcp-server",
    "name": "Dify MCP Server",
    "description": "Model Context Protocol (MCP) Server for dify workflows",
    "tags": [
        "mcp",
        "dify"
    ],
    "repo": "https://github.com/YanxingLiu/dify-mcp-server",
    "command": "uv",
    "baseArgs": [
        "--directory",
        "${DIFY_MCP_SERVER_PATH}",
        "run",
        "dify_mcp_server"
    ],
    "env": {
        "CONFIG_PATH": "$CONFIG_PATH"
    },
    "configurable": true,
    "configSchema": {
        "properties": {
            "dify_base_url": {
                "type": "string",
                "description": "The base URL for Dify API",
                "required": true
            },
            "dify_app_sks": {
                "type": "array",
                "description": "List of Dify application SKs",
                "required": true,
                "minItems": 1
            }
        }
    },
    "argsMapping": {
        "configPath": {
            "type": "env",
            "key": "CONFIG_PATH"
        }
    }
}