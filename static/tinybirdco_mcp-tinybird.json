{
    "id": "mcp-tinybird",
    "name": "MCP Tinybird",
    "description": "An MCP server to interact with a Tinybird Workspace from any MCP client.",
    "tags": [
        "tinybird",
        "mcp",
        "server"
    ],
    "repo": "https://github.com/tinybirdco/mcp-tinybird",
    "command": "uv",
    "baseArgs": [
        "mcp-tinybird"
    ],
    "configurable": true,
    "configSchema": {
        "properties": {
            "TB_API_URL": {
                "type": "string",
                "description": "Tinybird API URL",
                "required": true
            },
            "TB_ADMIN_TOKEN": {
                "type": "string",
                "description": "Tinybird Admin Token",
                "required": true
            }
        }
    },
    "argsMapping": {
        "TB_API_URL": {
            "type": "env",
            "key": "TB_API_URL"
        },
        "TB_ADMIN_TOKEN": {
            "type": "env",
            "key": "TB_ADMIN_TOKEN"
        }
    }
}